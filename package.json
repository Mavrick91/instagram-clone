{
  "name": "instagram-clone",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node server.mjs",
    "dev-debug": "NODE_OPTIONS='--inspect' next",
    "build": "next build",
    "start": "set NODE_ENV=production&& node server.mjs",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write .",
    "lint:fix": "eslint --fix .",
    "check": "tsc --noEmit",
    "seed": "node prisma/seed.mjs",
    "clear-s3": "node script/clearS3Bucket.mjs",
    "drop-db": "set PGPASSWORD=postgres&& psql -h localhost -U postgres -c \"DROP DATABASE IF EXISTS \\\"instagram-clone-db\\\"\"",
    "create-db": "set PGPASSWORD=postgres&& psql -h localhost -U postgres -c \"CREATE DATABASE \\\"instagram-clone-db\\\"\"",
    "reset": "npm run drop-db && npm run create-db && npm run clear-s3 && npx prisma db push && npx prisma generate && npm run seed"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.600.0",
    "@faker-js/faker": "^8.4.1",
    "@hookform/resolvers": "^3.3.4",
    "@mantine/core": "^7.11.0",
    "@mantine/hooks": "^7.11.0",
    "@prisma/client": "^5.15.0",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@tanstack/react-query": "^5.45.0",
    "@tanstack/react-query-devtools": "^5.45.0",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dotenv": "^16.4.5",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "framer-motion": "^11.2.10",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.395.0",
    "moment": "^2.30.1",
    "next": "^14.2.4",
    "prisma": "^5.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.2",
    "react-router-dom": "^6.22.3",
    "react-spinners": "^0.13.8",
    "react-textarea-autosize": "^8.5.3",
    "react-toastify": "^10.0.5",
    "sass": "^1.77.5",
    "serve": "^14.2.1",
    "sharp": "^0.33.4",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "autoprefixer": "^10.4.19",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.4",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-tailwindcss": "^3.17.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.1.5",
    "typescript": "5.5.3"
  }
}
